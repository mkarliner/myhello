	{
		"#": "Python Node",
		"input": {
			"bucket": "hello"
		},
		"implementation": {
			"python": "server/node2.python"
		},
        "outputs": {
          "who": {}
        }
	}],
    "stores":{
      "hello" : {
        "key$schema":"string"
      }
